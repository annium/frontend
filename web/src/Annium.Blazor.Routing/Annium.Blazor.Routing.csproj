        <PackageReference Include="Annium.Core.Mapper" Version="0.1.0" />
        <PackageReference Include="Annium.Core.Primitives" Version="0.1.0" />
        <PackageReference Include="Annium.Core.Runtime" Version="0.1.0" />
        <PackageReference Include="Annium.Data.Models" Version="0.1.0" />
        <PackageReference Include="Annium.Net.Base" Version="0.1.0" />
        <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.2" />
        <PackageReference Include="Microsoft.AspNetCore.WebUtilities" Version="2.2.0" />
    </ItemGroup>
</Project>